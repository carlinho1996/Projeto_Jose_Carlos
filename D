#include <iostream>
using namespace std;


void MD1(float D1, float D2){
	float k=0;
	k=(D1/D2)*100;
	cout << "DivisÃ£o de 2 numero em 100%= "<<k<<endl;
};

void MD2(float D1, float D2){
	float k=0;
	k=(D1+D2);
	cout <<"Media= "<<k<<endl;
};


int main(){
    float D1=2; 
    float D2=3;
        MD1(D1,D2);
        MD2(D1,D2);
return 0;
 }